geom_histogram(aes(y = ..density..), bins = 30, fill = "lightblue", color = "black") +
stat_function(fun = dnorm, args = list(mean = mean(Promedio), sd = sd(Promedio)),
color = "red", size = 1) +
labs(title = "Histograma de Promedio con Curva de Densidad Normal",
x = "Promedio",
y = "Densidad") +
theme_minimal()
